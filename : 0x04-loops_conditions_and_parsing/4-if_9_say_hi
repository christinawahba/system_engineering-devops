#!/usr/bin/env bash
# A Bash script that displays "Best School" 10 times, with a special message on the 9th iteration.

COUNT=1
while [ "$COUNT" -le 10 ]; do
    if [ "$COUNT" -eq 9 ]; then
        echo "Best School"
        echo "Hi"
    else
        echo "Best School"
    fi
    COUNT=$((COUNT + 1))
done
